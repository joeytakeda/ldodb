{% extends 'base.html.twig' %}

{% block title %}Supplemental Place Data{% endblock %}

{% block pageheader %}
    <h1>Supplemental Place Data</h1>
{% endblock %}

{% block body %}
    <div class="btn-toolbar">
        <div class="btn-group pull-right">
            <a href="{{ path('supplemental_place_data_index') }}" class="btn btn-default">
                <span class="glyphicon glyphicon-arrow-left"></span> Back
            </a>
            {% if is_granted('ROLE_CONTENT_ADMIN') %}
                <a href="{{ path('supplemental_place_data_edit', { 'id': supplementalPlaceDatum.id }) }}" class="btn btn-default">
                    <span class='glyphicon glyphicon-edit'></span> Edit
                </a>
                <a href="{{ path('supplemental_place_data_delete', { 'id': supplementalPlaceDatum.id }) }}" class="btn btn-default" data-confirm="Are you sure you want to delete this supplemental place data?">
                    <span class='glyphicon glyphicon-remove'></span> Delete
                </a>
            {% endif %}
        </div>
    </div>
    <table class="table table-bordered table-condensed table-hover table-striped">
        <tbody><tr>
                <th>Geoname ID</th>
                <td>{{ supplementalPlaceDatum.geonameId }}</td>
            </tr><tr>
                <th>Geoname</th>
                <td>{{ supplementalPlaceDatum.geoname }}</td>
            </tr><tr>
                <th>Latitude</th>
                <td>{{ supplementalPlaceDatum.latitude }}</td>
            </tr><tr>
                <th>Longitude</th>
                <td>{{ supplementalPlaceDatum.longitude }}</td>
            </tr>
        </tbody>
    </table>
{% endblock %}
